Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
!!Xft.autohint: true
Xft.hintstyle: hintslight
!Xft.lcdfilter: lcddefault
Xcursor.theme:Vanilla-DMZ

*.font:monospace:pixelsize=16
!*.fontalt0:Symbola:pixelsize=18
*.fontalt0:Symbola:pixelsize=12:antialias=true:autohint=true


*.background: #fafafa
*.foreground: #212121
*.cursor:     #212121


! black
*.color0:  #616161
*.color8:  #757575
! red
*.color1:  #ff1744
*.color9:  #ff5252
! green
*.color2:  #388e3c
*.color10: #43a047
! yellow
*.color3:  #e65100
*.color11: #ef6c00
! blue
*.color4:  #2979ff
*.color12: #448aff
! magenta
*.color5:  #d500f9
*.color13: #e040fb
! cyan
*.color6:  #0097a7
*.color14: #89DDFF
! white
*.color7:  #bdbdbd
*.color15: #e0e0e0


! 2bwm

twobwm.border_width: 0
twobwm.outer_border: 0
twobwm.focus_color: #515151
twobwm.unfocus_color: #f2f0ec
twobwm.fixed_color: #6699cc
twobwm.unkill_color: #66cccc
twobwm.outer_border_color: #F2F0EC
twobwm.fixed_unkill_color: #cc99cc
twobwm.inverted_colors: false
twobwm.enable_compton: true
